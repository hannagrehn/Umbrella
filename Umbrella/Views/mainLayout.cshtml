@using Umbraco.Cms.Web.Common.PublishedModels;
@inherits Umbraco.Cms.Web.Common.Views.UmbracoViewPage
@{
	Layout = null;
	var keywordsList = Model.Value<IEnumerable<string>>("pageKeywords");
	var keywords = keywordsList != null ? string.Join(", ", keywordsList) : "";
	var pageTitle = !string.IsNullOrEmpty(ViewBag.Title) ? $"{ViewBag.Title} | Onatrix - Very Financial Solution, Sorta Business" : "Onatrix - Very Financial Solution, Sorta Business";
	
}
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" href="https://kit.fontawesome.com/a576612222.css" crossorigin="anonymous"> 
    <link rel="stylesheet" href="/css/main.css">  

    <title>@pageTitle</title>
	<meta name="description" content="@Model.Value("pageDescription")">
	<meta name="keywords" content="@keywords">
</head>

<body>
	<div class="wrapper">
		<header></header>
		<main>
			@RenderBody()
		</main>
		<footer></footer>
	</div>


	<script src="/scripts/main.js"></script>
</body>
</html>

